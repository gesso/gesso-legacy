{
  "name": "@gesso/gesso",
  "author": "Michael Gubbels",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gesso/gesso.git"
  },
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "build:docker2": "docker build -t gesso/gesso .",
    "build:docker": "docker build --tag gesso/gesso-host .",
    "start": "node ./dist/index.js",
    "queue:install": "docker run --name gesso-queue -p 127.0.0.1:4369:4369 -p 127.0.0.1:5671:5671 -p 127.0.0.1:5672:5672 -p 127.0.0.1:25672:25672 -p 127.0.0.1:15671:15671 -p 127.0.0.1:15672:15672 --hostname gesso-queue-host -e RABBITMQ_DEFAULT_USER=guest -e RABBITMQ_DEFAULT_PASS=guest -d rabbitmq:3.7.5-management",
    "queue:start": "docker start gesso-queue",
    "queue:stop": "docker stop gesso-queue",
    "queue:uninstall": "docker rm gesso-queue",
    "cache:install": "docker run --name gesso-cache -p 127.0.0.1:6379:6379 -d redis:4.0.9",
    "cache:start": "docker start gesso-cache",
    "cache:stop": "docker stop gesso-cache",
    "cache:uninstall": "docker rm gesso-cache",
    "clean": "rm -rfd dist && rm -rfd node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "amqplib": "^0.5.3",
    "chalk": "^2.4.2",
    "chalk-pipe": "^2.0.0",
    "command-line-args": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "inquirer": "^6.4.1",
    "ioredis": "^4.11.1",
    "nodemon": "^1.19.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.6.2",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/chalk-pipe": "^2.0.0",
    "@types/command-line-args": "^5.0.0",
    "@types/inquirer": "^6.0.3",
    "@types/ioredis": "^4.0.13",
    "@types/winston": "^2.4.4",
    "@types/amqp": "^0.2.4",
    "@types/amqplib": "^0.5.12",
    "@types/node": "^10.12.0",
    "@types/uuid": "^3.4.5"
  }
}
